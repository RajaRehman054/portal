{
    "version": 2,
    "routes": [
        {
            "src": "/(.*)",
            "dest": "/api/index.php"
        }
    ],
    "env": {
        "APP_ENV": "production",
        "APP_DEBUG": "false"
    },
    "functions": {
        "api/index.php": {
            "runtime": "vercel-php@0.6.0"
        }
    }
}